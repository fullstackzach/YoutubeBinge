{
  "name": "codetest",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "concurrently": "4.1.2",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.0",
    "react-scripts": "3.4.2"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, gulp-watch\" --prefix name \"npm run start\" \"gulp watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/.eslintrc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
